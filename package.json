{
    "private": true,
    "name": "softeng-startercode-d25",
    "version": "1.0.1",
    "scripts": {
        "dev": "yarn run dotenv -e .env -- turbo run dev",
        "migrate:local": "dotenv -e env/.env.local -c ./env./.env.local -- turbo run migrate --env-mode=loose",
        "migrate:wpi": "dotenv -e env/.env.local -c ./env./.env.wpi -- turbo run migrate --env-mode=loose",
        "migrate:prod": "dotenv -e env/.env.local -c ./env./.env.prod -- turbo run migrate --env-mode=loose",
        "generate": "dotenv -e env/.env.local -c ./env./.env.prod -- turbo run generate --env-mode=loose",
        "db": "docker-compose -f ./compose-files/docker-compose.db.yaml up --build",
        "dev:stop": "docker-compose -f ./compose-files/docker-compose.dev.yaml down --volumes --rmi local",
        "deploy": "docker-compose -f ./compose-files/docker-compose.prod.yaml up --build -d",
        "deploy:stop": "docker-compose -f ./compose-files/docker-compose.prod.yaml down",
        "setup": "yarn install && yarn build:dev",
        "fix": "yarn cache clean && yarn install",
        "test": "yarn run vitest --ui --open=false",
        "test:docker": "docker-compose -f compose-files/docker-compose.test.yaml up --build --renew-anon-volumes -d",
        "test:stop": "docker-compose -f compose-files/docker-compose.dev.yaml down --volumes --rmi local",
        "lint": "turbo run lint",
        "lint:fix": "turbo run lint:fix",
        "build": "turbo run build",
        "docker:upload": "turbo run docker:upload --concurrency=1",
        "generate_sdk": "yarn install && yarn dlx @yarnpkg/sdks base"
    },
    "devDependencies": {
        "@types/node": "^22.13.13",
        "@vitest/ui": "^3.0.9",
        "@yarnpkg/pnpify": "^4.1.4",
        "eslint": "^9.23.0",
        "eslint-config-custom": "workspace:*",
        "husky": "^9.1.7",
        "prettier": "^3.5.3",
        "prettier-config-custom": "workspace:*",
        "ts-config-custom": "workspace:*",
        "turbo": "*",
        "typescript": "^5.8.2",
        "vitest": "^3.0.9"
    },
    "dependencies": {
        "common": "workspace:*",
        "dotenv": "^16.4.7",
        "dotenv-cli": "^7.4.4"
    },
    "resolutions": {
        "dotenv": "^16.4.7",
        "dotenv-cli": "^7.4.4",
        "@types/node": "^22.13.13",
        "husky": "^9.1.7",
        "prettier": "^3.5.3",
        "typescript": "^5.8.2",
        "eslint": "^9.23.0",
        "vitest": "^3.0.9"
    },
    "workspaces": [
        "apps/*",
        "configs/*",
        "packages/*"
    ],
    "packageManager": "yarn@4.7.0+sha512.5a0afa1d4c1d844b3447ee3319633797bcd6385d9a44be07993ae52ff4facabccafb4af5dcd1c2f9a94ac113e5e9ff56f6130431905884414229e284e37bb7c9"
}
