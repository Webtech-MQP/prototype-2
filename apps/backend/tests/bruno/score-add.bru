meta {
  name: score-add
  type: http
  seq: 2
}

post {
  url: http://localhost:3001/api/score
  body: json
  auth: none
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "time": "{{currentTime}}",
    "score": 100
  }
}

script:pre-request {
  const currentDateTime = new Date().toISOString();
  bru.setEnvVar('currentTime', currentDateTime);
}
